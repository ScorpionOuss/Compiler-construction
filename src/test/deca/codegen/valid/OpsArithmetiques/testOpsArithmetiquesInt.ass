; start main program
	TSTO #2
	BOV stack_overflow_error
; Main program
	ADDSP #2
	LOAD #3, R2
	STORE R2, 1(GB)
	LOAD #0, R2
	STORE R2, 2(GB)
; Beginning of main instructions:
	LOAD 1(GB), R2
	ADD #7, R2
	LOAD #3, R3
	CMP #0, R3
	BEQ ZeroDivision_Error
	LOAD 1(GB), R3
	QUO #3, R3
	SUB R3, R2
	MUL #5, R2
	STORE R2, 1(GB)
	WSTR "x = "
	LOAD 1(GB), R2
	LOAD R2, R1
	WINT
	WNL
	WSTR "division entière: "
	LOAD #2, R2
	CMP #0, R2
	BEQ ZeroDivision_Error
	LOAD 1(GB), R2
	QUO #2, R2
	LOAD R2, R1
	WINT
	WNL
	WSTR "reste: "
	LOAD #2, R2
	CMP #0, R2
	BEQ ZeroDivision_Error
	LOAD 1(GB), R2
	REM #2, R2
	LOAD R2, R1
	WINT
	HALT
stack_overflow_error:
	WSTR "Error: Stack Overflow"
	WNL
	ERROR
io_error:
	WSTR "Error: Input/Output error"
	WNL
	ERROR
ArithFloat_Error:
	WSTR "Error: Débordement arithmétique sur flottants"
	WNL
	ERROR
ZeroDivision_Error:
	WSTR "Error: Division entière par 0"
	WNL
	ERROR
; end main program

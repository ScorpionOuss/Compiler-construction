; start main program
	TSTO #2
	BOV stack_overflow_error
	ADDSP #3
	LOAD #null, R0
	STORE R0, 1(GB)
	LOAD code.Object.equals, R0
	STORE R0, 2(GB)
; Main program
	ADDSP #2
	LOAD #3, R2
	STORE R2, 3(GB)
	LOAD #0, R2
	STORE R2, 4(GB)
; Beginning of main instructions:
	LOAD 3(GB), R3
	ADD #7, R3
	LOAD #3, R4
	CMP #0, R4
	BEQ ZeroDivision_Error
	LOAD 3(GB), R4
	QUO #3, R4
	SUB R4, R3
	MUL #5, R3
	STORE R3, 3(GB)
	WSTR "x = "
	LOAD 3(GB), R2
	LOAD R2, R1
	WINT
	WNL
	WSTR "division entière: "
	LOAD #2, R2
	CMP #0, R2
	BEQ ZeroDivision_Error
	LOAD 3(GB), R2
	QUO #2, R2
	LOAD R2, R1
	WINT
	WNL
	WSTR "reste: "
	LOAD #2, R2
	CMP #0, R2
	BEQ ZeroDivision_Error
	LOAD 3(GB), R2
	REM #2, R2
	LOAD R2, R1
	WINT
	HALT
stack_overflow_error:
	WSTR "Error: Stack Overflow"
	WNL
	ERROR
io_error:
	WSTR "Error: Input/Output error"
	WNL
	ERROR
ArithFloat_Error:
	WSTR "Error: Débordement arithmétique sur flottants"

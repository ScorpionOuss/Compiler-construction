; start main program
	TSTO #3
	BOV stack_overflow_error
; Main program
	ADDSP #2
	LOAD #3, R2
	STORE R2, 1(LB)
	LOAD #0, R2
	STORE R2, 2(LB)
; Beginning of main instructions:
	LOAD 1(LB), R2
	ADD #7, R2
	PUSH R2
	LOAD 1(LB), R2
	LOAD #3, R2
	CMP #0, R2
	BEQ ZeroDivision_Error
	QUO #3, R2
	LOAD R2, R1
	POP R2
	SUB R1, R2
	MUL #5, R2
	STORE R2, 1(LB)
	WSTR "x = "
	LOAD 1(LB), R2
	LOAD R2, R1
	WINT
	WNL
	WSTR "division entière: "
	LOAD 1(LB), R2
	LOAD #2, R2
	CMP #0, R2
	BEQ ZeroDivision_Error
	QUO #2, R2
	LOAD R2, R1
	WINT
	WNL
	WSTR "reste: "
	LOAD #2, R2
	CMP #0, R2
	BEQ ZeroDivision_Error
	LOAD 1(LB), R2
	REM #2, R2
	LOAD R2, R1
	WINT
	HALT
stack_overflow_error:
	WSTR "Error: Stack Overflow"
	WNL
	ERROR
io_error:

; start main program
	TSTO #1
	BOV stack_overflow_error
	ADDSP #3
	LOAD #null, R0
	STORE R0, 1(GB)
	LOAD code.Object.equals, R0
	STORE R0, 2(GB)
; Main program
	ADDSP #1
	LOAD #0x1.8p1, R2
	STORE R2, 3(GB)
; Beginning of main instructions:
	LOAD 3(GB), R3
	LOAD #7, R4
	FLOAT R4, R4
	ADD R4, R3
	BOV ArithFloat_Error
	LOAD 3(GB), R4
	LOAD 3(GB), R4
	LOAD #3, R5
	FLOAT R5, R5
	DIV R5, R4
	BOV ArithFloat_Error
	SUB R4, R3
	BOV ArithFloat_Error
	LOAD #5, R4
	FLOAT R4, R4
	MUL R4, R3
	BOV ArithFloat_Error
	STORE R3, 3(GB)
	WSTR "x = "
	LOAD 3(GB), R2
	LOAD R2, R1
	WFLOAT
	WNL
	WSTR "division entière: "
	LOAD 3(GB), R2
	DIV #0x1.0p1, R2
	BOV ArithFloat_Error
	LOAD R2, R1
	WFLOAT
	HALT
stack_overflow_error:
	WSTR "Error: Stack Overflow"
	WNL
	ERROR
io_error:
	WSTR "Error: Input/Output error"
	WNL
	ERROR
ArithFloat_Error:
	WSTR "Error: Débordement arithmétique sur flottants"
	WNL
	ERROR
ZeroDivision_Error:
	WSTR "Error: Division entière par 0"
	WNL
	ERROR
dereferencement.null:
	WSTR "Erreur : dereferencement de null"
	WNL
	ERROR
tas_plein:
	WSTR "Erreur : heap_over_flow"
	WNL
	ERROR
code.Object.equals:
	RTS
init.object:
	RTS
; end main program

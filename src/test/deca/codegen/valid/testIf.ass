; start main program
	TSTO #1
	BOV stack_overflow_error
; Main program
	ADDSP #1
	LOAD #3, R2
	STORE R2, 1(GB)
; Beginning of main instructions:
	LOAD #5, R2
	CMP 1(GB), R2
	BLE E_Sinon.1
	WSTR """5 >"""
	LOAD 1(GB), R2
	LOAD R2, R1
	WINT
	WNL
	BRA E_Fin.1
E_Sinon.1:
	LOAD 1(GB), R2
	LOAD R2, R1
	WINT
	WSTR """  <= 5"""
	WNL
E_Fin.1:
	HALT
stack_overflow_error:
	WSTR "Error: Stack Overflow"
	WNL
	ERROR
; end main program

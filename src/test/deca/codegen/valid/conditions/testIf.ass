; start main program
	TSTO #1
	BOV stack_overflow_error
; Main program
	ADDSP #1
	LOAD #3, R2
	STORE R2, 1(GB)
; Beginning of main instructions:
	LOAD #5, R2
	CMP 1(GB), R2
	BLE E_Sinon.1
	LOAD #7, R2
	MUL #2, R2
	CMP #7, R2
	BLE E_Sinon.2
	WSTR "oui"
	WNL
	BRA E_Fin.2
E_Sinon.2:
E_Fin.2:
	BRA E_Fin.1
E_Sinon.1:
	WSTR "non"
	WNL
E_Fin.1:
	HALT
stack_overflow_error:
	WSTR "Error: Stack Overflow"
	WNL
	ERROR
io_error:
	WSTR "Error: Input/Output error"
	WNL
	ERROR
ArithFloat_Error:
	WSTR "Error: Débordement arithmétique sur flottants"
	WNL
	ERROR
ZeroDivision_Error:
	WSTR "Error: Division entière par 0"
	WNL
	ERROR
; end main program

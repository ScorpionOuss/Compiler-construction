; start main program
	TSTO #2
	BOV stack_overflow_error
; Main program
	ADDSP #2
	LOAD #1, R2
	STORE R2, 1(LB)
	LOAD #0x1.19999ap0, R2
	STORE R2, 2(LB)
; Beginning of main instructions:
	LOAD #1, R2
	CMP #0, R2
	BLT E_Sinon.1
	WSTR "ok"
	WNL
	LOAD 1(LB), R2
	LOAD R2, R1
	WINT
	WNL
	LOAD 2(LB), R2
	LOAD R2, R1
	WFLOAT
	WNL
	BRA E_Fin.1
E_Sinon.1:
	WSTR "erreur"
	WNL
E_Fin.1:
	WSTR "ok"
	LOAD 1(LB), R2
	LOAD R2, R1
	WINT
	LOAD 2(LB), R2
	LOAD R2, R1
	WFLOAT
	WNL
	HALT
stack_overflow_error:
	WSTR "Error: Stack Overflow"
	WNL
	ERROR
io_error:

	STORE R0, 9(GB)
	LOAD code.A.m, R0
	STORE R0, 9(GB)
	LOAD code.Object.equals, R0
	STORE R0, 7(GB)
; Main program
	ADDSP #1
	NEW #3, R2
	BOV tas_plein
	LEA 3(GB), R0
	STORE R0, 0(R2)
	PUSH R2
	BSR init.A
	POP R2
	STORE R2, 1(LB)
; Beginning of main instructions:
	LOAD 1(LB), R2
	BEQ dereferencement.null
	LOAD #2, R3
	STORE R3, 1(R2)

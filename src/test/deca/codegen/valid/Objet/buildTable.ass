; start main program
	TSTO #1
	BOV stack_overflow_error
	ADDSP #12
	LOAD #null, R0
	STORE R0, 1(GB)
	LOAD code.Object.equals, R0
	STORE R0, 2(GB)
	ADDSP #3
	LEA 1(GB), R0
	STORE R0, 3(GB)
	LOAD code.Object.equals, R0
	STORE R0, 4(GB)
	LOAD code.A.m, R0
	STORE R0, 5(GB)
	LOAD code.A.p, R0
	STORE R0, 6(GB)
	ADDSP #4
	LEA 3(GB), R0
	STORE R0, 7(GB)
	LOAD code.Object.equals, R0
	STORE R0, 8(GB)
	LOAD code.A.m, R0
	STORE R0, 9(GB)
	LOAD code.B.p, R0
	STORE R0, 10(GB)
	LOAD code.B.q, R0
	STORE R0, 11(GB)
; Main program
	ADDSP #1
	NEW #3, R2
	BOV tas_plein
	LEA 3(GB), R0
	STORE R0, 0(R2)
	PUSH R2
	BSR init.A
	POP R2
	STORE R2, 12(GB)
; Beginning of main instructions:
	LOAD 12(GB), R2
	BEQ dereferencement.null
	LOAD #1, R3
	STORE R3, 1(R2)
	LOAD 12(GB), R2
	BEQ dereferencement.null
	LOAD 1(R2), R2
	CMP #0, R2
	BEQ E_Sinon.1
	WSTR "Et ben oui"
	WNL
	BRA E_Fin.1
E_Sinon.1:
E_Fin.1:
	ADDSP #2
	LOAD 12(GB), R2
	STORE R2, 0(SP)
	LOAD #3, R2
	STORE R2, -1(SP)
	LOAD 0(SP), R2
	BEQ dereferencement.null
	LOAD 0(R2), R2
	BSR 2(R2)
	LOAD R0, R2
	SUBSP #2
	CMP #0, R2
	BEQ E_Sinon.2
	WSTR "et ben oui encore une fois!"
	BRA E_Fin.2
E_Sinon.2:
E_Fin.2:
	HALT
stack_overflow_error:
	WSTR "Error: Stack Overflow"
	WNL
	ERROR
io_error:
	WSTR "Error: Input/Output error"
	WNL
	ERROR
ArithFloat_Error:
	WSTR "Error: Débordement arithmétique sur flottants"
	WNL
	ERROR
ZeroDivision_Error:
	WSTR "Error: Division entière par 0"
	WNL
	ERROR
dereferencement.null:
	WSTR "Erreur : dereferencement de null"
	WNL
	ERROR
tas_plein:
	WSTR "Erreur : heap_over_flow"
	WNL
	ERROR
init.A:
	TSTO #1
	BOV stack_overflow_error
	PUSH R2
	LOAD -2(LB), R1
	PUSH R1
	BSR init.Object
	SUBSP #1
	LOAD -2(LB), R1
	STORE R2, 1(R1)
	LOAD #2, R2
	LOAD -2(LB), R1
	STORE R2, 2(R1)
	POP R2
	RTS
code.A.m:
	TSTO #1
	BOV stack_overflow_error
	ADDSP #1
	LOAD #1, R2
	STORE R2, 1(LB)
	PUSH R2
	PUSH R3
	LOAD -2(LB), R2
	BEQ dereferencement.null
	LOAD #2, R3
	STORE R3, 2(R2)
	LOAD -2(LB), R3
	LOAD 2(R3), R3
	ADD #2, R3
	ADD -3(LB), R3
	STORE R3, 1(LB)
	LOAD 1(LB), R2
	LOAD R2, R1
	WINT
	WNL
	WSTR "Ben oui ça marche!"
	WNL
	LOAD -2(LB), R2
	LOAD 1(R2), R2
	LOAD R2, R0
	BRA end.A.m
	LOAD -2(LB), R2
	LOAD 1(R2), R2
	LOAD R2, R0
	BRA end.A.m
	WSTR "Erreur : sortie de la methode A.getX sans return"
	WNL
	ERROR
end.A.m:
	POP R3
	POP R2
	RTS
code.A.p:
	TSTO #0
	BOV stack_overflow_error
	PUSH R2
end.A.p:
	POP R2
	RTS
init.B:
	TSTO #1
	BOV stack_overflow_error
	PUSH R2
	LOAD -2(LB), R1
	PUSH R1
	BSR init.A
	SUBSP #1
	LOAD -2(LB), R1
	STORE R2, 3(R1)
	LOAD -2(LB), R1
	STORE R2, 4(R1)
	POP R2
	RTS
code.B.p:
	TSTO #0
	BOV stack_overflow_error
	PUSH R2
end.B.p:
	POP R2
	RTS
code.B.q:
	TSTO #0
	BOV stack_overflow_error
	PUSH R2
end.B.q:
	POP R2
	RTS
code.Object.equals:
	RTS
init.object:
	RTS
; end main program

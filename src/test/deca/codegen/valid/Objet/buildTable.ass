; start main program
	TSTO #1
	BOV stack_overflow_error
	LOAD #null, R0
	STORE R0, 1(GB)
	LOAD code.Object.equals, R0
	STORE R0, 2(GB)
	LEA 1(GB), R0
	STORE R0, 3(GB)
	LOAD code.A.p, R0
	STORE R0, 6(GB)
	LOAD code.A.m, R0
	STORE R0, 6(GB)
	LOAD code.Object.equals, R0

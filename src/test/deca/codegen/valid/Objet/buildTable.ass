	LOAD -3(LB), R3
	STORE R3, 1(R2)
	LOAD -3(LB), R2
	LOAD R2, R0
	BRA end.A.m
	LOAD -3(LB), R2
	LOAD R2, R0
	BRA end.A.m
	WSTR "Erreur : sortie de la methode A.getX sans return"
	WNL
	ERROR
end.A.m:
	POP R3
	POP R2
	RTS
code.A.p:
	TSTO #0
	BOV stack_overflow_error
	PUSH R2
end.A.p:
	POP R2
	RTS
init.B:
	TSTO #1
	BOV stack_overflow_error
	PUSH R2
	LOAD -2(LB), R1
	PUSH R1
	BSR init.A
	SUBSP #1
	LOAD -2(LB), R1
	STORE R2, 1(R1)
	LOAD -2(LB), R1

	LOAD #1, R2
	LOAD -2(LB), R1
	STORE R2, 1(R1)
	LOAD #2, R2
	LOAD -2(LB), R1
	STORE R2, 2(R1)
	POP R2
	RTS
code.A.m:
	TSTO #0
	BOV stack_overflow_error
	PUSH R2
	PUSH R3

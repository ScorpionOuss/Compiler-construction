; start main program
	TSTO #2
	BOV stack_overflow_error
; Main program
	ADDSP #2
	RFLOAT
	BOV io_error
	LOAD R1, R2
	STORE R2, 1(GB)
	RINT
	BOV io_error
	LOAD R1, R2
	STORE R2, 2(GB)
; Beginning of main instructions:
	LOAD 1(GB), R2
	LOAD R2, R1
	WFLOAT
	HALT
stack_overflow_error:
	WSTR "Error: Stack Overflow"
	WNL
	ERROR
io_error:
	WSTR "Error: Input/Output error"
	WNL
	ERROR
; end main program

; start main program
	TSTO #6
	BOV stack_overflow_error
	ADDSP #2
	LOAD #null, R0
	STORE R0, 1(GB)
	LOAD code.Object.equals, R0
	STORE R0, 2(GB)
; Main program
	ADDSP #4
	LOAD #3, R2
	STORE R2, 3(GB)
	LOAD #0, R2
	STORE R2, 4(GB)
	LOAD #0x1.8p1, R2
	STORE R2, 5(GB)
	LOAD #0x1.0p1, R2
	STORE R2, 6(GB)
; Beginning of main instructions:
	WSTR "tests opérations arithmétiques sur entiers"
	WNL
	LOAD 3(GB), R3
	ADD #7, R3
	LOAD #3, R4
	CMP #0, R4
	BEQ ZeroDivision_Error
	LOAD 3(GB), R4
	QUO #3, R4
	SUB R4, R3
	MUL #5, R3
	STORE R3, 3(GB)
	LOAD 3(GB), R2
	WSTR "x = "
	LOAD 3(GB), R2
	LOAD R2, R1
	WINT
	WNL
	WSTR "division entière: "
	LOAD #2, R2
	CMP #0, R2
	BEQ ZeroDivision_Error
	LOAD 3(GB), R2
	QUO #2, R2
	LOAD R2, R1
	WINT
	WNL
	WSTR "reste: "
	LOAD #2, R2
	CMP #0, R2
	BEQ ZeroDivision_Error
	LOAD 3(GB), R2
	REM #2, R2
	LOAD R2, R1
	WINT
	WSTR "tests opérations arithmétiques sur flottants"
	WNL
	LOAD 5(GB), R3
	LOAD #7, R4
	FLOAT R4, R4
	ADD R4, R3
	BOV ArithFloat_Error
	LOAD 5(GB), R4
	LOAD 5(GB), R4
	LOAD #3, R5
	FLOAT R5, R5
	DIV R5, R4
	BOV ArithFloat_Error
	SUB R4, R3
	BOV ArithFloat_Error
	LOAD #5, R4
	FLOAT R4, R4
	MUL R4, R3
	BOV ArithFloat_Error
	STORE R3, 5(GB)
	LOAD 5(GB), R2
	WSTR "z = "
	LOAD 5(GB), R2
	LOAD R2, R1
	WFLOAT
	WNL
	WSTR "division entière: "
	LOAD 5(GB), R2
	DIV #0x1.0p1, R2
	BOV ArithFloat_Error
	LOAD R2, R1
	WFLOAT
	WSTR "Test conversion implicite"
	WNL
	LOAD 6(GB), R2
	LOAD #2, R3
	FLOAT R3, R3
	CMP R3, R2
	BNE E_Sinon.1
	WSTR "Oui! On teste la division: y2/2 = "
	LOAD 6(GB), R2
	LOAD 6(GB), R2
	LOAD #2, R3
	FLOAT R3, R3
	DIV R3, R2

	LOAD #2, R2
	STORE R2, 1(GB)
; Beginning of main instructions:
	LOAD #3, R2
	CMP 1(GB), R2
	BGT endAnd.1
	LOAD 1(GB), R2
	CMP #5, R2
	BLT E_Sinon.1
endAnd.1:
